{
	"name": "darren-glanville/filament-slider-widget",
	"description": "A basic slider for filament powered by Swiper JS.",
	"keywords": [
		"dg-dev",
		"darren-glanville",
		"filament-slider-widget"
	],
	"homepage": "https://github.com/darren-glanville/filament-slider-widget",
	"license": "MIT",
	"type": "library",
	"authors": [
		{
			"name": "Darren Glanville",
			"email": "hithere@darren-glanville.dev"
		}
	],
	"require": {
		"php": "^7.4|^8.0",
		"illuminate/support": "^8.0"
	},
	"require-dev": {
		"orchestra/testbench": "^6.0",
		"phpunit/phpunit": "^9.0"
	},
	"autoload": {
		"psr-4": {
			"DgDev\\WidgetSlider\\": "src"
		}
	},
	"config": {
		"sort-packages": true
	},
	"extra": {
		"laravel": {
			"providers": [
				"DgDev\\WidgetSlider\\WidgetSliderServiceProvider"
			]
		}
	}
}
