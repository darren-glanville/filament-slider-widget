{
	"name": "darren-glanville/filament-slider-widget",
	"description": "A basic slider for filament powered by Swiper JS.",
	"keywords": [
		"dg-dev",
		"darren-glanville",
		"filament-slider-widget"
	],
	"homepage": "https://github.com/darren-glanville/filament-slider-widget",
	"license": "MIT",
	"type": "library",
	"authors": [
		{
			"name": "Darren Glanville",
			"email": "hithere@darren-glanville.dev"
		}
	],
	"require": {
		"php": "^8.1",
		"filament/filament": "^3.0",
		"spatie/laravel-package-tools": "^1.15.0",
		"illuminate/support": "^10.0"
	},
	"autoload": {
		"psr-4": {
			"DgDev\\SliderWidget\\": "src"
		}
	},
	"config": {
		"sort-packages": true
	},
	"extra": {
		"laravel": {
			"providers": [
				"DgDev\\SliderWidget\\SliderWidgetServiceProvider"
			]
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
